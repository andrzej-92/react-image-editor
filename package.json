{
  "name": "react-image-editor",
  "version": "0.0.1",
  "description": "A react app for manipulate images through html 5 canvas",
  "main": "src/index.tsx",
  "scripts": {
    "clean": "rimraf build/",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack -p --bail --progress",
    "build:prod": "npm run clean && webpack -p --bail --progress --hide-modules --env production",
    "start": "webpack-dev-server --progress --open --inline --hot",
    "typings": "rimraf typings/ && typings install",
    "postinstall": "npm run typings"
  },
  "keywords": [
    "react",
    "canvas-image",
    "image-editor"
  ],
  "author": "Andrzej Tracz",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^7.0.13",
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.24.1",
    "babel-loader": "^6.0.0",
    "babel-plugin-transform-regenerator": "^6.24.1",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "compression-webpack-plugin": "^0.4.0",
    "css-loader": "^0.28.0",
    "cssnext-loader": "^1.0.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "immutable": "^3.8.1",
    "node-sass": "^4.5.2",
    "open": "0.0.5",
    "postcss": "^5.2.17",
    "postcss-cssnext": "^2.10.0",
    "postcss-loader": "^1.3.3",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-hot-loader": "^1.3.1",
    "react-input-range": "^1.1.2",
    "react-redux": "^5.0.4",
    "react-toolbox": "^2.0.0-beta.8",
    "redux": "^3.6.0",
    "redux-saga": "^0.14.8",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "ts-loader": "^2.0.3",
    "typescript": "^2.2.2",
    "typings": "^2.1.1",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.4",
    "yargs": "^7.1.0"
  },
